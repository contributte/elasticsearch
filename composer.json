{
  "name": "contributte/elasticsearch",
  "description": "Tiny Elasticsearch integration into Nette Framework",
  "keywords": [
    "contributte",
    "nette",
    "elasticsearch",
    "search",
    "elastic",
    "es"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/elasticsearch",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "elasticsearch/elasticsearch": "^8.11.0",
    "nette/di": "^3.1.8"
  },
  "require-dev": {
    "contributte/phpstan": "~0.1.0",
    "contributte/qa": "~0.4.0",
    "contributte/tester": "~0.4.0"
  },
  "conflict": {
    "psr/http-message": "<2.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Elasticsearch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
